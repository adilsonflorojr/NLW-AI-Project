generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Video {
  id          String   @id @default(uuid())
  nome        String
  caminho     String
  transcricao String?
  data        DateTime @default(now())
}

model CLI {
  id     String @id @default(uuid())
  titulo String
  texto  String
}
